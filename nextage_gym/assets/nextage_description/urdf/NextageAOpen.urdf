  <xacro:include filename="$(find nextage_gym/assets/nextage_description)/urdf/materials.urdf.xacro" />
  <material name="mLightGrey">
    <color rgba="0.6 0.6 0.6 1.0"/>
  </material>

  <xacro:macro name="gazebo_nx_link" params="name color">
    <gazebo reference="gazebo_${name}">
      <material>Gazebo/${color}</material>
    </gazebo>
  </xacro:macro>

  <xacro:macro name="transmission_nx_joint" params="name">
    <transmission name="${name}_Trans">
      <type>transmission_interface/SimpleTransmission</type>
      <joint name="${name}">
        <hardwareInterface>EffortJointInterface</hardwareInterface>
      </joint>
      <actuator name="${name}_Motor">
        <hardwareInterface>EffortJointInterface</hardwareInterface>
        <mechanicalReduction>1</mechanicalReduction>
      </actuator>
    </transmission>
  </xacro:macro>

